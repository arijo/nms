.modal .content .popup .body{color:#aaa;background-color:#fff;}.modal .content .popup .body iframe{width:100%;height:100%;border:none;}
footer #sonybug{text-shadow:none;filter:dropshadow(enabled=false);float:right;position:absolute;right:25px;bottom:40px;background:url(../images/logo-sonymakebelieve.png) no-repeat right;z-index:1015;}footer #sonybug img{text-shadow:none;filter:dropshadow(enabled=false);border:none;}
footer #sonybug a,footer #sonybug a:hover{text-shadow:none;filter:dropshadow(enabled=false);padding:0;margin-right:80px;border:none;background:none;}
footer #copyright-text{font-family:Chivo,Helvetica,"Helvetica Neue",Arial,"Open Sans",sans-serif;color:white;float:right;position:absolute;right:25px;bottom:15px;font-size:10px;text-align:right;padding-top:50px;z-index:1010;}
@media (max-width:479px){footer #copyright-text{right:5px;bottom:5px;}}
